{
  "manifest_version": 3,
  "name": "Horse Wallpaper Extension",
  "version": "1.0",
  "description": "Display a random horse image on new tab.",
  "permissions": ["tabs", "storage", "unlimitedStorage"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/tab.png",
    "48": "icons/tab.png",
    "128": "icons/tab.png"
  }
}
